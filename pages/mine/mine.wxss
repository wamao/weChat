

page{
  background:#f5f5f5;
}


.userInfo{
  background: #fff;
  flex-direction: row;
  background:#d81e06;
  display:flex;
  padding:20px 10px;
  width:90%;
  border-radius: 5px;
  margin:10px auto;
  align-items: center;

}

.userInfo>image{
  width: 70px;
  height:70px;
  border-radius: 35px;
}

.username{
  display: flex;
  flex-direction: column;
  margin-left:20px;
}
.username text{
  color:#fff;
  font-size: 14px;
}

.set{
  display: flex;
  flex-direction: row;
  margin-top:5px;
  align-items: center;
}
.set image{
  width:20px;
  height:20px;
  margin-right: 5px;
}

.set text{
  color:#e6e6e6;
}
.itemcontent{
   background: #fff;
  box-sizing: border-box;
  padding-left:15px;
  margin-top:1px;
}
.item{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  
  height:45px;
  padding-right:10px;
  box-sizing: border-box;
}

.item_left{
 display: flex;
 flex-direction: row;
}

.item_left image{
  width: 20px;
  height:20px;
  margin-right: 8px;
}

.item_left text{
  color:#2c2c2c;
  font-size: 14px;
  font-weight: 600;
}

.item_right{
  width:20px;
  height:20px;
}

.border{
border-bottom: 1px solid #ccc;
}